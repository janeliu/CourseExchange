
    <legend>Sign up</legend>
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <label><%= f.label :username %></label>
  <%= f.text_field :username, :autofocus => true %>

  <label><%= f.label :email %></label>
  <%= f.email_field :email %>

  <label><%= f.label :password %></label>
  <%= f.password_field :password %>

  <label><%= f.label :password_confirmation %></label>
  <%= f.password_field :password_confirmation %>

  <label><%= f.label :college_name, "College" %></label>
  <%= f.text_field :college_name, data: {autocomplete_source: College.order(:name).map(&:name)} %>
  

  <div><%= f.submit "Sign up" %></div>
<% end %>


